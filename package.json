{
  "name": "dash-web",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "start": "ng serve -o",
    "start:dev": "npm run start -- -c=dev",
    "build": "ng build --configuration=production",
    "build:bundle-stats": "npm run build -- --stats-json",
    "test": "jest --config jest-config.ts",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "code:check": "prettier . --check",
    "code:format": "prettier . --write",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "bundle:analyze": "esbuild-visualizer --metadata ./public/stats.json --filename ./bundle-stats/index.html"
  },
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  },
  "dependencies": {
    "@angular/animations": "20.2.4",
    "@angular/cdk": "20.2.2",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/material": "20.2.2",
    "@angular/material-date-fns-adapter": "20.2.2",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/router": "20.2.4",
    "@angular/service-worker": "20.2.4",
    "@compodoc/compodoc": "1.1.30",
    "chart.js": "4.5.0",
    "date-fns": "4.1.0",
    "jwt-decode": "4.0.0",
    "leaflet": "1.9.4",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "8.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.2.2",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@eslint/js": "9.35.0",
    "@types/jest": "29.5.14",
    "@types/leaflet": "1.9.20",
    "@types/node": "24.3.1",
    "angular-calendar": "0.32.0",
    "cypress": "14.5.4",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "2.0.5",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-cypress": "5.1.1",
    "globals": "16.4.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-preset-angular": "14.6.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "5.0.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.6.2",
    "stylelint": "16.24.0",
    "stylelint-config-standard-scss": "15.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
  }
}
