{
  "name": "Dash-Web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run Dash-Web:e2e-ci",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "marge tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "13.3.6",
    "@angular/cdk": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/material": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "chart.js": "3.7.1",
    "date-fns": "2.28.0",
    "jwt-decode": "3.1.2",
    "ng2-charts": "3.0.10",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.5",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.6",
    "@cypress/schematic": "1.6.0",
    "@ngneat/spectator": "10.0.1",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "angular-calendar": "0.29.0",
    "cypress": "9.6.1",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.0",
    "cypress-wait-until": "1.7.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "gh-pages": "3.2.3",
    "jest": "27.5.1",
    "jest-date-mock": "1.0.8",
    "jest-preset-angular": "11.1.2",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "2.6.2",
    "stylelint": "14.8.2",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4"
  }
}
