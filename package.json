{
  "name": "Dash-Web",
  "version": "0.4.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "start": "ng serve",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run Dash-Web:e2e-ci",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "dependencies": {
    "@angular/animations": "16.2.1",
    "@angular/cdk": "16.2.1",
    "@angular/common": "16.2.1",
    "@angular/compiler": "16.2.1",
    "@angular/core": "16.2.1",
    "@angular/forms": "16.2.1",
    "@angular/material": "16.2.1",
    "@angular/platform-browser": "16.2.1",
    "@angular/platform-browser-dynamic": "16.2.1",
    "@angular/router": "16.2.1",
    "chart.js": "4.3.3",
    "date-fns": "2.30.0",
    "jwt-decode": "3.1.2",
    "leaflet": "1.9.4",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "5.0.3",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "16.2.1",
    "@compodoc/compodoc": "1.1.21",
    "@cypress/schematic": "2.5.0",
    "@ngneat/spectator": "15.0.1",
    "@types/jest": "29.5.3",
    "@types/leaflet": "1.9.3",
    "@types/node": "18.17.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "angular-calendar": "0.31.0",
    "cypress": "12.17.4",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.3",
    "cypress-wait-until": "2.0.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.14.0",
    "gh-pages": "6.0.0",
    "jest": "29.6.3",
    "jest-date-mock": "1.0.8",
    "jest-preset-angular": "13.1.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.0.2",
    "stylelint": "15.10.3",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "10.0.0",
    "typescript": "5.1.6"
  }
}