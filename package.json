{
  "name": "Dash-Web",
  "version": "0.4.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "start": "ng serve",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run Dash-Web:e2e-ci",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "dependencies": {
    "@angular/animations": "15.2.7",
    "@angular/cdk": "15.2.7",
    "@angular/common": "15.2.7",
    "@angular/compiler": "15.2.7",
    "@angular/core": "15.2.7",
    "@angular/forms": "15.2.7",
    "@angular/material": "15.2.7",
    "@angular/platform-browser": "15.2.7",
    "@angular/platform-browser-dynamic": "15.2.7",
    "@angular/router": "15.2.7",
    "chart.js": "4.2.1",
    "date-fns": "2.29.3",
    "jwt-decode": "3.1.2",
    "leaflet": "1.9.3",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "4.1.1",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.6",
    "@angular/cli": "15.2.6",
    "@angular/compiler-cli": "15.2.7",
    "@compodoc/compodoc": "1.1.19",
    "@cypress/schematic": "2.5.0",
    "@ngneat/spectator": "14.0.0",
    "@types/jest": "29.5.0",
    "@types/leaflet": "1.9.3",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "angular-calendar": "0.31.0",
    "cypress": "12.9.0",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.3",
    "cypress-wait-until": "1.7.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.2",
    "gh-pages": "5.0.0",
    "jest": "29.5.0",
    "jest-date-mock": "1.0.8",
    "jest-preset-angular": "13.0.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "2.8.7",
    "stylelint": "15.4.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "8.0.0",
    "typescript": "4.9.5"
  }
}