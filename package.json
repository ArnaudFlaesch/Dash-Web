{
  "name": "Dash-Web",
  "version": "0.4.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "ng serve -o",
    "start:dev": "npm run start -- -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,html}\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "dependencies": {
    "@angular/animations": "18.2.1",
    "@angular/cdk": "18.2.1",
    "@angular/common": "18.2.1",
    "@angular/compiler": "18.2.1",
    "@angular/core": "18.2.1",
    "@angular/forms": "18.2.1",
    "@angular/material": "18.2.1",
    "@angular/material-date-fns-adapter": "18.2.1",
    "@angular/platform-browser": "18.2.1",
    "@angular/platform-browser-dynamic": "18.2.1",
    "@angular/router": "18.2.1",
    "@angular/service-worker": "18.2.1",
    "@compodoc/compodoc": "1.1.25",
    "chart.js": "4.4.3",
    "date-fns": "3.6.0",
    "jwt-decode": "4.0.0",
    "leaflet": "1.9.4",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "6.0.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.1",
    "@angular/cli": "18.2.1",
    "@angular/compiler-cli": "18.2.1",
    "@eslint/js": "9.9.0",
    "@types/jest": "29.5.12",
    "@types/leaflet": "1.9.12",
    "@types/node": "20.14.15",
    "angular-calendar": "0.31.1",
    "cypress": "13.13.3",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.4",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "3.5.0",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-preset-angular": "14.2.2",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.3.3",
    "stylelint": "16.8.1",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.1.0"
  }
}
