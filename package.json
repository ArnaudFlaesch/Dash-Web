{
  "name": "Dash-Web",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "ng serve -o",
    "start:dev": "npm run start -- -c=dev",
    "build": "ng build --configuration=production",
    "build:bundle-stats": "npm run build -- --stats-json",
    "test": "jest --config jest-config.mjs",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "code:check": "prettier . --check",
    "code:format": "prettier . --write",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "bundle:analyze": "esbuild-visualizer --metadata ./public/stats.json --filename ./bundle-stats/index.html"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "dependencies": {
    "@angular/animations": "19.1.3",
    "@angular/cdk": "19.1.1",
    "@angular/common": "19.1.3",
    "@angular/compiler": "19.1.3",
    "@angular/core": "19.1.3",
    "@angular/forms": "19.1.3",
    "@angular/material": "19.1.1",
    "@angular/material-date-fns-adapter": "19.1.1",
    "@angular/platform-browser": "19.1.3",
    "@angular/platform-browser-dynamic": "19.1.3",
    "@angular/router": "19.1.3",
    "@angular/service-worker": "19.1.3",
    "@compodoc/compodoc": "1.1.26",
    "chart.js": "4.4.7",
    "date-fns": "4.1.0",
    "jwt-decode": "4.0.0",
    "leaflet": "1.9.4",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "8.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.4",
    "@angular/cli": "19.1.4",
    "@angular/compiler-cli": "19.1.3",
    "@eslint/js": "9.19.0",
    "@types/jest": "29.5.14",
    "@types/leaflet": "1.9.16",
    "@types/node": "22.10.10",
    "angular-calendar": "0.31.1",
    "cypress": "14.0.0",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "2.0.4",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-cypress": "4.1.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-preset-angular": "14.5.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.4.2",
    "stylelint": "16.14.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0"
  }
}
