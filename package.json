{
  "name": "Dash-Web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "ng serve",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run Dash-Web:e2e-ci",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "marge tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "dependencies": {
    "@angular/animations": "15.0.2",
    "@angular/cdk": "14.2.7",
    "@angular/common": "15.0.2",
    "@angular/compiler": "15.0.2",
    "@angular/core": "15.0.2",
    "@angular/forms": "15.0.2",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "15.0.2",
    "@angular/platform-browser-dynamic": "15.0.2",
    "@angular/router": "15.0.2",
    "chart.js": "4.0.1",
    "date-fns": "2.29.3",
    "jwt-decode": "3.1.2",
    "leaflet": "1.9.3",
    "leaflet-sidebar-v2": "3.2.3",
    "ng2-charts": "4.1.1",
    "rxjs": "7.6.0",
    "tslib": "2.4.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.2",
    "@angular/cli": "15.0.2",
    "@angular/compiler-cli": "15.0.2",
    "@compodoc/compodoc": "1.1.19",
    "@cypress/schematic": "2.4.0",
    "@ngneat/spectator": "12.2.1",
    "@tailwindcss/line-clamp": "0.4.2",
    "@types/jest": "28.1.8",
    "@types/leaflet": "1.9.0",
    "@types/node": "18.11.11",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "angular-calendar": "0.30.1",
    "cypress": "12.0.1",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.1",
    "cypress-wait-until": "1.7.2",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "gh-pages": "4.0.0",
    "jest": "28.1.3",
    "jest-date-mock": "1.0.8",
    "jest-preset-angular": "12.2.3",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "2.8.1",
    "stylelint": "14.16.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "6.1.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  }
}